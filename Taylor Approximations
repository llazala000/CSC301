   "source": [
    "import sympy as sy\n",
    "from sympy.functions import sin,cos\n",
    "import math\n",
    "        \n",
    "nVals = [1, 2, 3, 4, 8, 16, 32, 64, 128, 256]\n",
    "xVals = [1/32, 1/16, 1/8, 1/4, 1/2, 1, 2, 4]\n",
    "exactVal = list()\n",
    "approxVal = list()\n",
    "AbsErr = list()\n",
    "RelErr = list()\n",
    "xlist = list()\n",
    "nlist = list()\n",
    "    \n",
    "def factorial(number):\n",
    "    if number < 2:\n",
    "        return 1\n",
    "    else:\n",
    "        return number * factorial(number-1)\n",
    "    \n",
    "def taylor_sin(x, n):\n",
    "    s = 0\n",
    "    exact_val = math.sin(x)\n",
    "    for k in range(n):\n",
    "        s = s + (((-1)**k)/factorial(2*k+1)*x**(2*k+1))\n",
    "    approxVal.append(s)\n",
    "    xlist.append(x)\n",
    "    nlist.append(n)\n",
    "    exactVal.append(exact_val)\n",
    "    abs_err = exact_val - s\n",
    "    AbsErr.append(abs_err)\n",
    "    RelErr.append(abs_err/s)\n",
    "    \n",
    "        \n",
    "\n",
    "for x in xVals:\n",
    "    for num in nVals:\n",
    "        taylor_sin(x, num)\n",
    "\n",
    "print(tabulate({\"x\": xlist, \n",
    "                \"n\":nlist,\n",
    "                \"Exact Value\":exactVal,\n",
    "                \"Taylor Approximation of Sin(x)\":approxVal,\n",
    "                \"Absolute Error\": AbsErr,\n",
    "                \"Relative Error\":RelErr},\n",
    "                headers=\"keys\"))\n"
   ]
  },
