   ],
   "source": [
    "from tabulate import tabulate\n",
    "import math\n",
    "from scipy.misc import factorial\n",
    "        \n",
    "nVals = [1, 2, 3, 4, 8, 16, 32, 64, 128, 256]\n",
    "xVals = [1/32, 1/16, 1/8, 1/4, 1/2, 1, 2, 4]\n",
    "exactVal = list()\n",
    "approxVal = list()\n",
    "AbsErr = list()\n",
    "RelErr = list()\n",
    "xlist = list()\n",
    "nlist = list()\n",
    "      \n",
    "def taylor_ln(x, n):\n",
    "    s = 0\n",
    "    exact_val = math.log(1+x)\n",
    "    for k in range(1, n):\n",
    "            s = s + ((-1)**(k+1))*((x**k)/k)\n",
    "    approxVal.append(s)\n",
    "    xlist.append(x)\n",
    "    nlist.append(n)\n",
    "    exactVal.append(exact_val)\n",
    "    abs_err = exact_val - s\n",
    "    AbsErr.append(abs_err)\n",
    "    if (s == 0):\n",
    "        RelErr.append(0)\n",
    "    else: \n",
    "        RelErr.append(abs_err/s)\n",
    "    \n",
    "        \n",
    "\n",
    "for x in xVals:\n",
    "    for num in nVals:\n",
    "        taylor_ln(x, num)\n",
    "\n",
    "print(tabulate({\"x\": xlist, \n",
    "                \"n\":nlist,\n",
    "                \"Exact Value\":exactVal,\n",
    "                \"Taylor Approximation of ln(1-x)\":approxVal,\n",
    "                \"Absolute Error\": AbsErr,\n",
    "                \"Relative Error\":RelErr},\n",
    "                headers=\"keys\"))"
   ]
  },
