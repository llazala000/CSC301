   "source": [
    "from tabulate import tabulate\n",
    "import math\n",
    "from scipy.misc import factorial #had to use this for this function because I kept getting error message: int too large to \n",
    "# convert to float. factorial function gets very large. scipy factorial uses gamma function approximation \n",
    "# to calculate the factorial so that we don't run into an overflow problem.\n",
    "        \n",
    "nVals = [1, 2, 3, 4, 8, 16, 32, 64, 128, 256]\n",
    "xVals = [1/32, 1/16, 1/8, 1/4, 1/2, 1, 2, 4]\n",
    "exactVal = list()\n",
    "approxVal = list()\n",
    "AbsErr = list()\n",
    "RelErr = list()\n",
    "xlist = list()\n",
    "nlist = list()\n",
    "\n",
    "def taylor_cos(x, n):\n",
    "    s = 0\n",
    "    exact_val = math.cos(x)\n",
    "    for k in range(n):\n",
    "        s = s + ((-1)**k)*(x**(2*k))/factorial(2*k)\n",
    "    approxVal.append(s)\n",
    "    xlist.append(x)\n",
    "    nlist.append(n)\n",
    "    exactVal.append(exact_val)\n",
    "    abs_err = exact_val - s\n",
    "    AbsErr.append(abs_err)\n",
    "    RelErr.append(abs_err/s)\n",
    "    \n",
    "        \n",
    "\n",
    "for x in xVals:\n",
    "    for num in nVals:\n",
    "        taylor_cos(x, num)\n",
    "\n",
    "print(tabulate({\"x\": xlist, \n",
    "                \"n\":nlist,\n",
    "                \"Exact Value\":exactVal,\n",
    "                \"Taylor Approximation of cos(x)\":approxVal,\n",
    "                \"Absolute Error\": AbsErr,\n",
    "                \"Relative Error\":RelErr},\n",
    "                headers=\"keys\"))"
   ]
  },
